<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Dashboard</title>
</head>
<body>
    <h1>Dashboard</h1>
    <% if (role === 'admin') { %>
        <ul>
            <li><a href="/students">Student Management</a></li>
            <li><a href="/faculty">Faculty Management</a></li>
            <li><a href="/courses">Course Scheduling</a></li>
            <li><a href="/enrollments">Enrollment Tracking</a></li>
        </ul>
    <% } else if (role === 'faculty') { %>
        <ul>
            <li><a href="/faculty/courses">Courses</a></li>
            <li><a href="/faculty/students">Enrolled Students</a></li>
        </ul>
    <% } else if (role === 'student') { %>
        <ul>
            <li><a href="/student/courses">My Courses</a></li>
            <li><a href="/student/enroll">Enroll in Course</a></li>
        </ul>
    <% } %>
</body>
</html>
